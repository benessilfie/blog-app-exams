<section class="mt-20">
    <%# render navbar partial from shared %>
    <%= render "shared/navbar" %>

    <%# render notice %>
    <% if notice.present? %>
    <p class="pt-2 pl-6 text-sm text-green-700 inline-block" id="notice">
        <%= notice %>
    </p>
    <% end %>

    <div id="blogs" >
        <% @blogs.each do |blog| %>
            <%= render blog %>
        <% end %>
    </div>
</section>
